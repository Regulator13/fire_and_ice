}
else if (hspeed &lt;= (-fric))
{hspeed += fric;
}
else if(abs(hspeed) &lt; fric)
{hspeed = 0
}
}
}
else {
//set not solid
if (frozen) {
hspeed = 0;
vspeed = 0;
if (place_free(x, y)) solid = true;
}
else if !(stuck) solid = false;
}

