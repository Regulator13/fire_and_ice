gravityI = 0.4;
fric = 2.4 //Set all objects friction

//playerInput
playerInput = 0;
// InputPlayer - player object where to get input from, local or network
InputPlayer = noone;

//inputBuffer - buffer for joystick input
inputBuffer = 0;
